            <!--Hero-->
            <section class="home-area -mt-24 pt-24 static">
                <div class="container px-4 sm:px-8 lg:grid lg:grid-cols-2 lg:gap-x-8">
                    <div class="md:pt-12">
                        <img class="lg:h-80 h-40 mx-auto jump rounded wow animate__animated animate__fadeInUp animated" src="https://carolinenoyer.fr/images/logo-nine-2021.png" alt="" style="visibility: visible; animation-name: jump;">
                    </div>
                    <div class="mb-16 mt-12 lg:mt-32 xl:mt-40 xl:mr-12">
                        <div class="max-w-2xl mx-auto mb-8">
                            <h2 class="text-3xl text-white lg:text-5xl lg:leading-normal mb-4 font-bold font-heading wow animate__ animate__fadeIn animated" style="visibility: visible; animation-name: fadeIn;">Passionnée par le <span class="text-turquoise">digital</span> depuis 2005</h2>
                            <p class="text-blue-gray-400 text-xl leading-relaxed wow animate__ animate__fadeIn animated" style="visibility: visible; animation-name: fadeIn;"><strong class="text-blue-500">Expérience</strong> en <span id="typedtext" class="text-white typewrite d-inline text-brand" data-period="3000" data-type="[&quot;Product Owner&quot;, &quot;UX Design&quot;, &quot;Développement web&quot; ]"></span></p>
                        </div>
                        <div class="max-w-2xl mx-auto">
                            <div class="text-blue-gray-400 p-3 text-center inline-flex items-center justify-center w-12 h-12 mb-5 shadow-lg rounded-full border-white border-2">
                                <a href="https://www.linkedin.com/in/carolinenoyer" title="LinkedIn"><i class="fa fa-linkedin"></i></a>
                            </div>
                            
                        </div>
                    </div>
                </div> <!-- end of container -->
            </section>
            <div class="relative bottom-0">
                <svg xmlns="http://www.w3.org/2000/svg" class="absolute left-0 w-full block" viewBox="0 0 1440 320" style="height:140px;top:-139px" preserveAspectRatio="none"><path class="fill-white" fill-opacity="1" d="M 0 288 L 80 288 C 160 288 320 288 480 245.3 C 640 203 800 117 960 96 C 1120 75 1280 117 1360 138.7 L 1440 160 L 1440 320 L 0 320 Z"></path></svg>
            </div>
                


        {% block body %}
        {{page}}
        <div style="height: 1000px;"></div>
        
        {% endblock %}
        

            <!-- Footer start -->
