            <script src="https://code.jquery.com/ui/1.13.2/jquery-ui.js"></script>
            <script src="https://kit.fontawesome.com/e13f84fe9c.js" crossorigin="anonymous"></script>
            
            {{ encore_entry_script_tags('app') }}
