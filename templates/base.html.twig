            <!--Hero-->
            <section class="home-area -mt-24 pt-24">
                <div class="container px-4 sm:px-8 lg:grid lg:grid-cols-2 lg:gap-x-8">
                    <div class="pt-12 text-center">
                        <img class="jump rounded wow animate__animated animate__fadeInUp animated" src="https://carolinenoyer.fr/images/logo-nine-2021.png" alt="" style="visibility: visible; animation-name: jump;">
                    </div>
                    <div class="mb-16 lg:mt-32 xl:mt-40 xl:mr-12">
                        <div class="max-w-2xl mx-auto mb-8">
                            <h2 class="text-3xl lg:text-5xl lg:leading-normal mb-4 font-bold font-heading wow animate__ animate__fadeIn animated" style="visibility: visible; animation-name: fadeIn;">Passionnée par le <span class="text-blue-500">digital</span> depuis 2005</h2>
                            <p class="text-blue-gray-400 leading-relaxed wow animate__ animate__fadeIn animated" style="visibility: visible; animation-name: fadeIn;"><strong class="text-blue-500">Expérience</strong> en <span class="typewrite d-inline text-brand" data-period="3000" data-type="[&quot;Product Owner&quot;, &quot;UX Design&quot;, &quot;Développement web&quot; ]"></span></p>
                        </div>
                        <div>
                            <a class="btn-primary py-4 px-8 mr-2 wow animate__ animate__fadeInUp hover-up-2 animated" href="https://wp.alithemes.com/html/monst/#key-features" style="visibility: visible; animation-name: fadeInUp;">Key Features</a>
                            <a class="btn-white wow animate__ animate__fadeInUp hover-up-2 animated" data-wow-delay=".3s" href="https://wp.alithemes.com/html/monst/#how-we-work" style="visibility: visible; animation-delay: 0.3s; animation-name: fadeInUp;">How We Work?</a>
                        </div>
                    </div>
                </div> <!-- end of container -->

                <div class="debug">
                <img src="https://carolinenoyer.fr/images/main/wave-shape.svg" class="bottom-shape img-fluid" alt="">
                </div>
                

            </section>
            <!--Features-->

        {% block body %}
        <div style="height: 1000px;"></div>
        {{page}}
        {% endblock %}
        

            <!-- Footer start -->
            <footer class="py-20 footer">
                <section class="py-20">
                    <div class="container px-4 mx-auto wow animate__animated animate__fadeIn" data-wow-delay=".3s" style="visibility: hidden; animation-delay: 0.3s; animation-name: none;">
                        <div class="flex flex-wrap mb-12 lg:mb-20 -mx-3 text-center lg:text-left">
                            <div class="w-full lg:w-1/5 px-3 mb-6 lg:mb-0">
                                <a class="inline-block mx-auto lg:mx-0 text-3xl font-semibold leading-none" href="https://wp.alithemes.com/html/monst/index.html">
                                    <img class="h-10" src="https://medias.carolinenoyer.fr/images/logo-2024-colored.png" alt="">
                                </a>
                            </div>
                            <div class="w-full lg:w-2/5 px-3 mb-8 lg:mb-0">
                                <p class="max-w-md mx-auto lg:max-w-full lg:mx-0 lg:pr-32 lg:text-lg text-blue-gray-400 leading-relaxed">Helping you <strong>maximize</strong> operations management with digitization</p>
                            </div>
                            <div class="w-full lg:w-1/5 px-3 mb-8 lg:mb-0">
                                <p class="mb-2 lg:mb-4 lg:text-lg text-smallcaps text-turquoise">Parcours</p>
                                <p class="lg:text-lg text-blue-gray-400">359 Hidden Valley Road, NY</p>
                            </div>
                            <div class="w-full lg:w-1/5 px-3">
                                <p class="mb-2 lg:mb-4 lg:text-lg text-smallcaps text-turquoise">Centres d'intérêt</p>
                                <p class="lg:text-lg text-blue-gray-400">(+01) 234 568</p>
                                <p class="lg:text-lg text-blue-gray-400">contact@monst.com</p>
                            </div>
                        </div>
                        <div class="flex flex-col lg:flex-row items-center lg:justify-between">
                            <p class="text-sm text-blue-gray-400">© 2021. All rights reserved. Designed by <a class="text-blue-400" href="https://alithemes.com/" target="_blank">Alithemes.com</a></p>
                            <div class="order-first lg:order-last -mx-2 mb-4 lg:mb-0">
                                <a class="inline-block px-2" href="https://wp.alithemes.com/html/monst/#">
                                    FB
                                </a>
                                <a class="inline-block px-2" href="https://wp.alithemes.com/html/monst/#">
                                    twitter
                                </a>
                                <a class="inline-block px-2" href="https://wp.alithemes.com/html/monst/#">
                                    instagram
                                </a>
                            </div>
                        </div>
                    </div>
                </section>
            </footer>
        </div>
