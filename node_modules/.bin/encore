#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Applications/MAMP/htdocs/portfolio/node_modules/.pnpm/@symfony+webpack-encore@4.6.1_@babel+core@7.24.4_@babel+preset-env@7.24.4_@symfony+stimulus-b_o2ivpd4xyarvsg42x6ldtqxv2y/node_modules/@symfony/webpack-encore/bin/node_modules:/Applications/MAMP/htdocs/portfolio/node_modules/.pnpm/@symfony+webpack-encore@4.6.1_@babel+core@7.24.4_@babel+preset-env@7.24.4_@symfony+stimulus-b_o2ivpd4xyarvsg42x6ldtqxv2y/node_modules/@symfony/webpack-encore/node_modules:/Applications/MAMP/htdocs/portfolio/node_modules/.pnpm/@symfony+webpack-encore@4.6.1_@babel+core@7.24.4_@babel+preset-env@7.24.4_@symfony+stimulus-b_o2ivpd4xyarvsg42x6ldtqxv2y/node_modules/@symfony/node_modules:/Applications/MAMP/htdocs/portfolio/node_modules/.pnpm/@symfony+webpack-encore@4.6.1_@babel+core@7.24.4_@babel+preset-env@7.24.4_@symfony+stimulus-b_o2ivpd4xyarvsg42x6ldtqxv2y/node_modules:/Applications/MAMP/htdocs/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Applications/MAMP/htdocs/portfolio/node_modules/.pnpm/@symfony+webpack-encore@4.6.1_@babel+core@7.24.4_@babel+preset-env@7.24.4_@symfony+stimulus-b_o2ivpd4xyarvsg42x6ldtqxv2y/node_modules/@symfony/webpack-encore/bin/node_modules:/Applications/MAMP/htdocs/portfolio/node_modules/.pnpm/@symfony+webpack-encore@4.6.1_@babel+core@7.24.4_@babel+preset-env@7.24.4_@symfony+stimulus-b_o2ivpd4xyarvsg42x6ldtqxv2y/node_modules/@symfony/webpack-encore/node_modules:/Applications/MAMP/htdocs/portfolio/node_modules/.pnpm/@symfony+webpack-encore@4.6.1_@babel+core@7.24.4_@babel+preset-env@7.24.4_@symfony+stimulus-b_o2ivpd4xyarvsg42x6ldtqxv2y/node_modules/@symfony/node_modules:/Applications/MAMP/htdocs/portfolio/node_modules/.pnpm/@symfony+webpack-encore@4.6.1_@babel+core@7.24.4_@babel+preset-env@7.24.4_@symfony+stimulus-b_o2ivpd4xyarvsg42x6ldtqxv2y/node_modules:/Applications/MAMP/htdocs/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@symfony/webpack-encore/bin/encore.js" "$@"
else
  exec node  "$basedir/../@symfony/webpack-encore/bin/encore.js" "$@"
fi
